<ManageAccountsWindow>:
    name: "manage_accounts"

	canvas:
		Rectangle:
			source: 'Images/grad1.jpg'
			size: self.size
			pos: self.pos


    FloatLayout:
        Label:
            text: "Remove and view accounts"
		Button:
            text: "< Back"
            size_hint: (.15, .05)
            pos_hint: {'top':1, 'left':1}
            on_release:
                root.manager.transition.direction = "right"
                root.manager.current = "admin_action"
